{
  "api": {
    "APIKeyManager": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "APIKeyManagerLambda"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    },
    "TodoAPI": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "TodoAPILambda"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "auth": {
    "miniEventAuth": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "APIKeyManagerLambda": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "APIKeyDB"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "TodoAPILambda": {
      "build": true,
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "APIKeyDB"
        },
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "todoListDB"
        },
        {
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ],
          "category": "storage",
          "resourceName": "TodoListItemDB"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "APIKeyDB": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    },
    "TodoListItemDB": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    },
    "todoListDB": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}